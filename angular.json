{
	"version": 1,
	"projects": {
		"api": {
			"root": "apps/api",
			"sourceRoot": "apps/api/src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@nrwl/node:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/api",
						"main": "apps/api/src/main.ts",
						"tsConfig": "apps/api/tsconfig.app.json",
						"assets": ["apps/api/src/assets"]
					},
					"configurations": {
						"production": {
							"optimization": true,
							"extractLicenses": true,
							"inspect": false,
							"fileReplacements": [
								{
									"replace": "apps/api/src/environments/environment.ts",
									"with": "apps/api/src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/node:execute",
					"options": {
						"buildTarget": "api:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/api/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["{workspaceRoot}/coverage/apps/api"],
					"options": {
						"jestConfig": "apps/api/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"common-ui-material": {
			"root": "libs/common/ui/material",
			"sourceRoot": "libs/common/ui/material/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["libs/common/ui/material/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
					"options": {
						"jestConfig": "libs/common/ui/material/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"core": {
			"root": "libs/core",
			"sourceRoot": "libs/core/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["libs/core/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["{workspaceRoot}/coverage/libs/core"],
					"options": {
						"jestConfig": "libs/core/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"environments": {
			"root": "libs/environments",
			"sourceRoot": "libs/environments/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["libs/environments/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["{workspaceRoot}/coverage/libs/environments"],
					"options": {
						"jestConfig": "libs/environments/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"login": {
			"projectType": "application",
			"root": "apps/login",
			"sourceRoot": "apps/login/src",
			"prefix": "s1m",
			"architect": {
				"build": {
					"builder": "@nrwl/angular:webpack-browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/login",
						"index": "apps/login/src/index.html",
						"main": "apps/login/src/main.ts",
						"polyfills": "apps/login/src/polyfills.ts",
						"tsConfig": "apps/login/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/login/src/favicon.ico",
							"apps/login/src/assets"
						],
						"styles": ["apps/login/src/styles.scss"],
						"scripts": [],
						"customWebpackConfig": {
							"path": "apps/login/webpack.config.js"
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/login/src/environments/environment.ts",
									"with": "apps/login/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "apps/login/webpack.prod.config.js"
							}
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@nrwl/angular:webpack-dev-server",
					"configurations": {
						"production": {
							"browserTarget": "login:build:production"
						},
						"development": {
							"browserTarget": "login:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "login:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/login/src/**/*.ts",
							"apps/login/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["{workspaceRoot}/coverage/apps/login"],
					"options": {
						"jestConfig": "apps/login/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"login-e2e": {
			"root": "apps/login-e2e",
			"sourceRoot": "apps/login-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/login-e2e/cypress.json",
						"devServerTarget": "login:serve:development",
						"tsConfig": "apps/login-e2e/tsconfig.json"
					},
					"configurations": {
						"production": {
							"devServerTarget": "login:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["apps/login-e2e/**/*.{js,ts}"]
					}
				}
			},
			"tags": [],
			"implicitDependencies": ["login"]
		},
		"stores-users": {
			"root": "libs/stores/users",
			"sourceRoot": "libs/stores/users/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": ["libs/stores/users/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["{workspaceRoot}/coverage/libs/stores/users"],
					"options": {
						"jestConfig": "libs/stores/users/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"user-management": {
			"projectType": "application",
			"root": "apps/user-management",
			"sourceRoot": "apps/user-management/src",
			"prefix": "s1m",
			"architect": {
				"build": {
					"builder": "@nrwl/angular:webpack-browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/user-management",
						"index": "apps/user-management/src/index.html",
						"main": "apps/user-management/src/main.ts",
						"polyfills": "apps/user-management/src/polyfills.ts",
						"tsConfig": "apps/user-management/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/user-management/src/favicon.ico",
							"apps/user-management/src/assets"
						],
						"styles": ["apps/user-management/src/styles.scss"],
						"stylePreprocessorOptions": {
							"includePaths": ["libs/assets/scss/"]
						},
						"scripts": [],
						"customWebpackConfig": {
							"path": "apps/user-management/webpack.config.js"
						},
						"aot": false,
						"vendorChunk": true,
						"extractLicenses": false,
						"buildOptimizer": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/user-management/src/environments/environment.ts",
									"with": "apps/user-management/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "apps/user-management/webpack.prod.config.js"
							}
						},
						"development": {}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@nrwl/angular:webpack-dev-server",
					"configurations": {
						"production": {
							"browserTarget": "user-management:build:production"
						},
						"development": {
							"browserTarget": "user-management:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"publicHost": "http://localhost:4200"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "user-management:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/user-management/src/**/*.ts",
							"apps/user-management/src/**/*.html"
						]
					},
					"outputs": ["{options.outputFile}"]
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": [
						"{workspaceRoot}/coverage/apps/user-management"
					],
					"options": {
						"jestConfig": "apps/user-management/jest.config.ts",
						"passWithNoTests": true
					}
				},
				"serve-mfe": {
					"builder": "nx:run-commands",
					"options": {
						"commands": [
							"nx serve login",
							"nx serve user-management"
						]
					}
				}
			},
			"tags": []
		},
		"user-management-e2e": {
			"root": "apps/user-management-e2e",
			"sourceRoot": "apps/user-management-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/user-management-e2e/cypress.json",
						"devServerTarget": "user-management:serve:development",
						"tsConfig": "apps/user-management-e2e/tsconfig.json"
					},
					"configurations": {
						"production": {
							"devServerTarget": "user-management:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"outputs": ["{options.outputFile}"],
					"options": {
						"lintFilePatterns": [
							"apps/user-management-e2e/**/*.{js,ts}"
						]
					}
				}
			},
			"tags": [],
			"implicitDependencies": ["user-management"]
		}
	}
}
