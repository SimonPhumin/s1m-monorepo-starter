{
	"name": "admin-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"implicitDependencies": ["admin"],
	"projectType": "application",
	"sourceRoot": "apps/admin-e2e/src",
	"tags": ["scope:admin-e2e"],
	"targets": {
		"e2e": {
			"configurations": {
				"no-serve": {
					"command": "APP=apps/admin-e2e npx cucumber-js --config cucumber.base.js"
				}
			},
			"executor": "nx:run-commands",
			"options": {
				"command": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'nx serve admin --configuration=mock' 'https://localhost:4202/' 'APP=apps/admin-e2e npx cucumber-js --config cucumber.base.js'"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"options": {
				"lintFilePatterns": ["apps/admin-e2e/**/*.{js,ts}"]
			},
			"outputs": ["{options.outputFile}"]
		},
		"lint-ci": {
			"executor": "@nx/eslint:lint",
			"options": {
				"format": "checkstyle",
				"lintFilePatterns": ["apps/admin-e2e/**/*.ts"],
				"outputFile": "./test-reports/apps/admin-e2e/eslint/checkstyle-result.xml"
			},
			"outputs": ["{options.outputFile}"]
		}
	}
}
